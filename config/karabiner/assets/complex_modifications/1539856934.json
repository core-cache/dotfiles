{
  "title": "For British Keyboard (Exchange £ and #)",
  "rules": [
    {
      "description": "Exchange £ shift-3 and # option-3",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "3",
            "modifiers": {
              "mandatory": [ "option" ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "3",
              "modifiers": [
                "left_shift"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "3",
            "modifiers": {
              "mandatory": [
                "shift"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "3",
              "modifiers": [
                "left_option"
              ]
            }
          ]
        }
      ]
    }
  ]
}
